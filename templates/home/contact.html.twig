
<div class="row valign-wrapper">
    <div class="col s12">
        <h4 class="center-align">Contact</h4>
    </div>
</div>
<div class="row">
    <div class="col s12 m4">
        <div class="mapouter">
            <div class="gmap_canvas">
                <iframe
                        width="600"
                        height="500"
                        id="gmap_canvas"
                        src="https://maps.google.com/maps?q=grandclement%20villeurbanne&t=&z=13&ie=UTF8&iwloc=&output=embed"
                       frameborder="0"
                       scrolling="no"
                       marginheight="0"
                       marginwidth="0"
                >
                </iframe>
            </div>
        </div>
    </div>
    <div class="col s12 m6 offset-m2">
        <table>
            <tr>
                <td>Phone</td>
                <td>{{ user.phone }}</td>
            </tr>
            <tr>
                <td>Email</td>
                <td>{{ user.email }}</td>
            </tr>
            <tr>
                <td>Github</td>
                <td><a href="{{ user.githubLink }}">{{ user.githubLink }}</a></td>
            </tr>
        </table>
    </div>
</div>